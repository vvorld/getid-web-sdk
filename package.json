{
  "name": "getid-web-sdk",
  "version": "1.2.2",
  "main": "./lib/bundle.js",
  "author": "Anna Teterina",
  "description": "GetId Web SDK",
  "license": "MIT",
  "homepage": "https://github.com/vvorld/getid-web-sdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvorld/getid-web-sdk.git"
  },
  "scripts": {
    "start": "webpack-dev-server --open --watch --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build-np": "rollup --config",
    "lint": "eslint ./src",
    "test": "jest ./test",
    "supportedBrowsers": "echo \"module.exports = $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "verbose": true,
    "clearMocks": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/mocks/styleMock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.5.0",
    "@material-ui/pickers": "^3.2.6",
    "@material-ui/styles": "latest",
    "@rollup/plugin-json": "^4.0.1",
    "browserslist": "^4.8.2",
    "clsx": "latest",
    "core-js": "^3.4.8",
    "date-fns": "^2.8.1",
    "html-react-parser": "^0.10.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "rollup": "^1.27.14",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.4",
    "@rollup/plugin-url": "^4.0.0",
    "@testing-library/react": "^9.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browserslist-useragent-regexp": "^1.3.3-beta",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.3.2",
    "depcheck": "^0.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.12.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-brunch": "^3.0.0",
    "terser-webpack-plugin": "^2.2.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "bugs": {
    "url": "https://github.com/vvorld/getid-web-sdk/issues"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "sdk"
  ]
}
